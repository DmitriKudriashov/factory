<p id="notice"><%= notice %></p>

<h1>Modelprices</h1>

<table>
  <thead>
    <tr>
      <th>Model</th>
      <th>Category</th>
      <th>Description</th>
      <th>Quantity</th>
      <th>Unit</th>
      <th>Unit price</th>
      <th>Currency</th>
      <th>Rate Curry</th>
      <th>Sum (cur-ry)</th>
      <th>Sum (usd)   </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @modelprices.each do |modelprice| %>
      <tr>
        <td><%= modelprice.modelset.name %>   </td>
        <td><%= modelprice.category.name %>   </td>
        <td><%= modelprice.description.name %></td>
        <td><%= modelprice.quantty %>         </td>
        <td><%= modelprice.unit.name %>       </td>
        <td><%= modelprice.unit_price %>      </td>
        <td><%= modelprice.currency.name %>   </td>
        <td><%= modelprice.ratecurry.name %>  </td>
        <td><%= modelprice.sum_curry %>       </td>
        <td><%= modelprice.sum_usd %>         </td>
        <td><%= link_to 'Show', modelprice %> </td>
        <td><%= link_to 'Edit', edit_modelprice_path(modelprice) %> </td>
        <td><%= link_to 'Destroy', modelprice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Modelprice', new_modelprice_path %>
